{
  "manifest_version": 2,
  "name": "GifBump",
  "version": "0.1",

  "icons": {
    "128": "icon.png"
  },

  "browser_action":{
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "options_page": "options.html",
  
  "permissions": [
  	"gcm"
	],

  "web_accessible_resources": [
    "*.woff2"
  ]
}
